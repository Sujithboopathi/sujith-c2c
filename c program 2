#include <stdio.h>

int main() {
    int A, B, C, D, E, F;
    printf("Enter six integers (A, B, C, D, E, F): ");
    scanf("%d %d %d %d %d %d", &A, &B, &C, &D, &E, &F);
    int numbers[] = {A, B, C, D, E, F};
    char *labels[] = {"A", "B", "C", "D", "E", "F"};

    printf("Fair pairs are:\n");
    for (int i = 0; i < 6; i++) {
        for (int j = i + 1; j < 6; j++) {
            int sum = numbers[i] + numbers[j];
            if (sum >= 3 && sum <= 6) {
                printf("%s%s\n", labels[i], labels[j]);
            }
        }
    }

    return 0;
}
